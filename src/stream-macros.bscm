(with-tests
 (define-macro stream#cons
   [|a b|
    `(##cons ,a (delay ,b))])
 (begin
   (let ((s (stream#cons 1 2)))
     (all?
      (list
       (equal? (##car s)
		 1)
       (equal? (force (##cdr s))
		 2))))))
